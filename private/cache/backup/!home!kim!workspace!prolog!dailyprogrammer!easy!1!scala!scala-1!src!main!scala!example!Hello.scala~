package example

import java.util.{ArrayList, Calendar, Scanner, Random}
import scala.collection.immutable.HashMap

object Hello extends Greeting with App {
  println(greeting)
  val test: HashMap[Nothing, Nothing] = new HashMap()
  val test2: Scanner = new Scanner("test")
  val test3: ArrayList[Nothing] = new ArrayList()
  val test4: Calendar = Calendar.getInstance()
  val test5 = new Random()

  def modMethod(i: Int) = i % 2 == 0

  def testing(print : String) = {
    println(print)
  }
}

trait Greeting {
  lazy val greeting: String = "hello"
}
